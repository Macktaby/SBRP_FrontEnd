<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.macktaby.sbrp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.ProjectManagementActivity" />
        <activity android:name=".view.CentralManagementActivity" />
        <activity android:name=".view.PackagesActivity" />
        <activity android:name=".view.AddNewPackageActivity" />
        <activity android:name=".view.EditPackageActivity" />
        <activity android:name=".view.PeopleActivity" />
        <activity android:name=".view.ProjectsActivity" />
        <activity android:name=".view.AddNewProjectActivity" />
        <activity android:name=".view.EditProjectActivity" />
        <activity android:name=".view.AddNewPersonActivity"></activity>
    </application>

</manifest>